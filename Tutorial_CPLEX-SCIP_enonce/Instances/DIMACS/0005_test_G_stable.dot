graph G {
  0[shape = octagon]
  1[shape = octagon, color=white]
  2[shape = octagon, color=white]
  3[shape = octagon, color=white]
  4[shape = octagon]
  "0"--"1" [color=white];
  "0"--"2" [color=white];
  "1"--"3" [color=white];
  "1"--"4" [color=white];
  "2"--"3" [color=white];
  "3"--"4" [color=white];
}
